prefix=/usr
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
targets=x11 broadway wayland

gtk_binary_version=3.0.0
gtk_host=armv7l-unknown-linux-gnueabihf

Name: GTK+
Description: GTK+ Graphical UI Library
Version: 3.14.9
Requires: gdk-3.0 atk >= 2.12.0 cairo >= 1.12.0 cairo-gobject >= 1.12.0 gdk-pixbuf-2.0 >= 2.30.0 gio-2.0 >= 2.41.2
Requires.private: atk atk-bridge-2.0 pangoft2 gio-unix-2.0 >= 2.41.2
Libs: -L${libdir} -lgtk-3 
Cflags: -I${includedir}/gtk-3.0 
